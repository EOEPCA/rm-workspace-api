{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run FastAPI App",
            "type": "debugpy",
            "request": "launch",
            "module": "uvicorn",
            "console": "integratedTerminal",
            "env": {
                "HARBOR_URL": "",
                "HARBOR_ADMIN_USERNAME": "",
                "HARBOR_ADMIN_PASSWORD": "",
                "PREFIX_FOR_NAME": "ws",
                "WORKSPACE_SECRET_NAME": "workspace",
                "KUBECONFIG": "${env:HOME}/.kube/config-eoepca-demo"
            },
            "args": [
                "workspace_api:app",
                "--reload",
                "--host",
                "0.0.0.0",
                "--port",
                "5000",
                "--log-level",
                "info"
            ]
        }
    ]
}